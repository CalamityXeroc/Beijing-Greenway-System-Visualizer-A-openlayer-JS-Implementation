# 后端环境变量示例
# 复制此文件为 .env.local 并填入实际值

# ========================================
# 数据库配置
# ========================================

DB_HOST=localhost
DB_PORT=5432
DB_NAME=greenway
DB_USER=postgres
DB_PASSWORD=your_postgres_password

# ========================================
# 服务器配置
# ========================================

PORT=3001
NODE_ENV=development

# ========================================
# CORS 配置
# ========================================

CORS_ORIGIN=http://localhost:5173

# ========================================
# 日志级别
# ========================================

LOG_LEVEL=debug
